local function ARAE_fake_script() -- Fake Script: StarterGui.HackerooOVRMenu.Main.PG1.Menu.hbsizetxt.LocalScript
    local script = Instance.new("LocalScript")
    script.Name = "LocalScript"
    script.Parent = Converted["_hbsizetxt"]
    local req = require
    local require = function(obj)
        local fake = fake_module_scripts[obj]
        if fake then
            return fake()
        end
        return req(obj)
    end

	local Value = script.Parent.Value
	Value.Value = 0.5
	
	while task.wait() do
		if Value.Value < 0.5 then
			Value.Value = 0.5
		end
		for i,v in pairs(game.Players:GetChildren()) do
			if v ~= game.Players.LocalPlayer then
				if v.Character then
					v.Character:WaitForChild("Head").Size = Vector3.new(Value.Value,Value.Value,Value.Value)
				end
			end
		end
		script.Parent.Text = "Hitbox Size - "..math.floor(Value.Value*(10^2))/(10^2)
	end
end
local function ZUIVWB_fake_script() -- Fake Script: StarterGui.HackerooOVRMenu.Main.PG2.Menu.PlrESP.toggle.LocalScript
    local script = Instance.new("LocalScript")
    script.Name = "LocalScript"
    script.Parent = Converted["_toggle4"]
    local req = require
    local require = function(obj)
        local fake = fake_module_scripts[obj]
        if fake then
            return fake()
        end
        return req(obj)
    end

	local button = script.Parent
	local on = false
	
	function createESP(player)
		if player.Character:WaitForChild("Torso"):FindFirstChild("BillboardGui") == nil then
			print(player)
			local plr = player.Character
			local bgui = Instance.new("BillboardGui")
			bgui.Parent = plr:WaitForChild("Torso") or plr:FindFirstChild("Torso")
			bgui.Size = UDim2.new(2,0,3,0)
			bgui.AlwaysOnTop = true
			local ESP = Instance.new("Frame")
			ESP.Parent = bgui
			ESP.Size = UDim2.new(1,0,1,0)
			ESP.BackgroundTransparency = 0.5
			ESP.BackgroundColor3 = Color3.new(0.3,0,1)
			ESP.BorderSizePixel = 1
			ESP.BorderColor3 = Color3.new(0.15,0,0.5)
		end
	end
	function updateESP(players)
		for i,v in pairs(players) do
			if v.Character then
				if v.Character:WaitForChild("Torso"):FindFirstChild("BillboardGui") then
					local plr = game.Workspace:FindFirstChild(v.Name)
					local ESP = plr:WaitForChild("Torso",math.huge):WaitForChild("BillboardGui"):WaitForChild("Frame")
					if on then
						ESP.Visible = true
					else
						ESP.Visible = false
					end
				end
			end
		end
	end
	
	button.MouseButton1Click:Connect(function()
		if on then
			on = false
			button.BackgroundColor3 = Color3.new(1,0.2,0.2)
		else
			on = true
			button.BackgroundColor3 = Color3.new(0.2,1,0.2)
		end
	end)
	
	while task.wait() do
		for i,v in pairs(game.Players:GetChildren()) do
			if v ~= game.Players.LocalPlayer then
				if v.Character then
					createESP(v)
				end
			end
		end
		updateESP(game.Players:GetChildren())
	end
end
local function QQBOUEK_fake_script() -- Fake Script: StarterGui.HackerooOVRMenu.Main.PG2.Menu.NPCESP.toggle.LocalScript
    local script = Instance.new("LocalScript")
    script.Name = "LocalScript"
    script.Parent = Converted["_toggle5"]
    local req = require
    local require = function(obj)
        local fake = fake_module_scripts[obj]
        if fake then
            return fake()
        end
        return req(obj)
    end

	local button = script.Parent
	local on = false
	
	function createNPCESP(player)
		if player:WaitForChild("Torso",math.huge):FindFirstChild("BillboardGui") == nil then
			print(player)
			local bgui = Instance.new("BillboardGui")
			bgui.Parent = player:WaitForChild("Torso")
			bgui.Size = UDim2.new(2,0,3,0)
			bgui.AlwaysOnTop = true
			local ESP = Instance.new("Frame")
			ESP.Parent = bgui
			ESP.Size = UDim2.new(1,0,1,0)
			ESP.BackgroundTransparency = 0.5
			ESP.BackgroundColor3 = Color3.new(0,0.3,1)
			ESP.BorderSizePixel = 1
			ESP.BorderColor3 = Color3.new(0,0.15,0.5)
		end
	end
	function updateNPCESP(players)
		for i,v in pairs(players) do
			if v then
				if v:WaitForChild("Torso",math.huge):FindFirstChild("BillboardGui") then
					local plr = v
					local ESP = plr:WaitForChild("Torso",math.huge):WaitForChild("BillboardGui"):WaitForChild("Frame")
					if on then
						ESP.Visible = true
					else
						ESP.Visible = false
					end
				end
			end
		end
	end
	
	button.MouseButton1Click:Connect(function()
		if on then
			on = false
			button.BackgroundColor3 = Color3.new(1,0.2,0.2)
		else
			on = true
			button.BackgroundColor3 = Color3.new(0.2,1,0.2)
		end
	end)
	
	while task.wait() do
		for i,v in pairs(game.Workspace.NPCs:GetChildren()) do
			createNPCESP(v)
		end
		updateNPCESP(game.Workspace.NPCs:GetChildren())
	end
end
local function UQAT_fake_script() -- Fake Script: StarterGui.HackerooOVRMenu.Main.PG2.Menu.ItemESP.toggle.LocalScript
    local script = Instance.new("LocalScript")
    script.Name = "LocalScript"
    script.Parent = Converted["_toggle6"]
    local req = require
    local require = function(obj)
        local fake = fake_module_scripts[obj]
        if fake then
            return fake()
        end
        return req(obj)
    end

	local button = script.Parent
	local on = false
	
	function createItemESP(player)
		if player:FindFirstChild("BillboardGui") == nil then
			print(player)
			local bgui = Instance.new("BillboardGui")
			bgui.Parent = player
			bgui.Size = UDim2.new(1,0,1,0)
			bgui.AlwaysOnTop = true
			local ESP = Instance.new("Frame")
			ESP.Parent = bgui
			ESP.Size = UDim2.new(1,0,1,0)
			ESP.BackgroundTransparency = 0.5
			ESP.BackgroundColor3 = Color3.new(1,0,0.3)
			ESP.BorderSizePixel = 1
			ESP.BorderColor3 = Color3.new(0.5,0,0.15)
		end
	end
	function updateItemESP(players)
		for i,v in pairs(players) do
			if v then
				if v:FindFirstChild("BillboardGui") then
					local plr = v
					local ESP = plr:WaitForChild("BillboardGui"):WaitForChild("Frame")
					if on then
						ESP.Visible = true
					else
						ESP.Visible = false
					end
				end
			end
		end
	end
	
	button.MouseButton1Click:Connect(function()
		if on then
			on = false
			button.BackgroundColor3 = Color3.new(1,0.2,0.2)
		else
			on = true
			button.BackgroundColor3 = Color3.new(0.2,1,0.2)
		end
	end)
	
	while task.wait() do
		for i,v in pairs(game.Workspace.Items:GetChildren()) do
			createItemESP(v)
		end
		updateItemESP(game.Workspace.Items:GetChildren())
	end
end
