local function MAGPQG_fake_script() -- Fake Script: StarterGui.HackerooOVRMenu.Main.prenextpg.PG#.LocalScript
    local script = Instance.new("LocalScript")
    script.Name = "LocalScript"
    script.Parent = Converted["_PG#"]
    local req = require
    local require = function(obj)
        local fake = fake_module_scripts[obj]
        if fake then
            return fake()
        end
        return req(obj)
    end

	local Value = script.Parent.Parent.Value
	local Main = script.Parent.Parent.Parent
	Value.Value = 1
	while task.wait() do
		if Value.Value < 1 then
			Value.Value = 1
		end
		if Value.Value > 2 then
			Value.Value = 2
		end
		if Value.Value == 1 then
			Main.PG1.Visible = true
			Main.PG2.Visible = false
		end
		if Value.Value == 2 then
			Main.PG1.Visible = false
			Main.PG2.Visible = true
		end
		script.Parent.Text = "PG. "..Value.Value
	end
end

