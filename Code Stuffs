-- Fake Module Scripts:

local fake_module_scripts = {}


-- Fake Local Scripts:

local function BLWO_fake_script() -- Fake Script: StarterGui.HackerooOVRMenu.Main.ColorPulse
    local script = Instance.new("LocalScript")
    script.Name = "ColorPulse"
    script.Parent = Converted["_Main"]
    local req = require
    local require = function(obj)
        local fake = fake_module_scripts[obj]
        if fake then
            return fake()
        end
        return req(obj)
    end

	local Menu = script.Parent
	local i = 0
	local ia = 0.005
	
	while task.wait() do
		i+=ia
		if i>1 then
			i=1
			ia=-0.005
		end
		if i<0 then
			i=0
			ia=0.005
		end
		Menu.BackgroundColor3 = Color3.new(i/3,0,i)
	end
end
local function CFRK_fake_script() -- Fake Script: StarterGui.HackerooOVRMenu.Main.PG1.Menu.lasertxt.toggle.LocalScript
    local script = Instance.new("LocalScript")
    script.Name = "LocalScript"
    script.Parent = Converted["_toggle"]
    local req = require
    local require = function(obj)
        local fake = fake_module_scripts[obj]
        if fake then
            return fake()
        end
        return req(obj)
    end

	local button = script.Parent
	local on = false
	local RightArm = game.Workspace:WaitForChild(game.Players.LocalPlayer.Name):WaitForChild("Right Arm")
	local Laser = Instance.new("Part")
	Laser.Parent = RightArm
	Laser.Size = Vector3.new(0.1,1000,0.1)
	Laser.CanCollide = false
	Laser.Massless = true
	Laser.Anchored = true
	Laser.Color = Color3.new(0.3,0,1)
	local Offset = CFrame.new(0,-500,0)
	
	button.MouseButton1Click:Connect(function()
		if on then
			on = false
			button.BackgroundColor3 = Color3.new(1,0.2,0.2)
		else
			on = true
			button.BackgroundColor3 = Color3.new(0.2,1,0.2)
		end
	end)
	
	while task.wait() do
		Laser.CFrame = RightArm.CFrame:ToWorldSpace(Offset)
		if on then
			Laser.Transparency = 0.5
		else
			Laser.Transparency = 1
		end
	end
end
local function FOXUUB_fake_script() -- Fake Script: StarterGui.HackerooOVRMenu.Main.PG1.Menu.flytxt.toggle.LocalScript
    local script = Instance.new("LocalScript")
    script.Name = "LocalScript"
    script.Parent = Converted["_toggle1"]
    local req = require
    local require = function(obj)
        local fake = fake_module_scripts[obj]
        if fake then
            return fake()
        end
        return req(obj)
    end

	local button = script.Parent
	local on = false
	local LeftArm = game.Workspace:WaitForChild(game.Players.LocalPlayer.Name):WaitForChild("Left Arm")
	local UIS = game:GetService("UserInputService")
	local Fly = Instance.new("Part")
	Fly.Parent = LeftArm
	Fly.Size = Vector3.new(2,2,0.5)
	Fly.CanCollide = true
	Fly.Massless = true
	Fly.Anchored = true
	Fly.Color = Color3.new(0.3,0,1)
	local Offset = CFrame.new(1.5,0,2.5)
	
	button.MouseButton1Click:Connect(function()
		if on then
			on = false
			button.BackgroundColor3 = Color3.new(1,0.2,0.2)
		else
			on = true
			button.BackgroundColor3 = Color3.new(0.2,1,0.2)
		end
	end)
	
	while task.wait() do
		if on then
			if UIS:IsGamepadButtonDown(Enum.UserInputType.Gamepad1,Enum.KeyCode.ButtonX) then
				Fly.CFrame = LeftArm.CFrame:ToWorldSpace(Offset)
			else
				Fly.CFrame = CFrame.new(0,-100,0)
			end
		else
			Fly.CFrame = CFrame.new(0,-100,0)
		end
	end
end
