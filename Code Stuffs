local function MFZAX_fake_script() -- Fake Script: StarterGui.HackerooOVRMenu.Main.PG1.Menu.plattxt.toggle.LocalScript
    local script = Instance.new("LocalScript")
    script.Name = "LocalScript"
    script.Parent = Converted["_toggle3"]
    local req = require
    local require = function(obj)
        local fake = fake_module_scripts[obj]
        if fake then
            return fake()
        end
        return req(obj)
    end

	local button = script.Parent
	local on = false
	local Wrkspc = game.Workspace
	local UIS = game:GetService("UserInputService")
	local lhand = game.Workspace:WaitForChild(game.Players.LocalPlayer.Name):WaitForChild("Left Arm")
	local rhand = game.Workspace:WaitForChild(game.Players.LocalPlayer.Name):WaitForChild("Right Arm")
	local offset = CFrame.new(0,0,0.5)
	local lplat = Instance.new("Part")
	lplat.Anchored = true
	lplat.CanCollide = true
	lplat.Massless = true
	lplat.Parent = lhand
	lplat.Size = Vector3.new(1,1,0.1)
	local rplat = Instance.new("Part")
	rplat.Anchored = true
	rplat.CanCollide = true
	rplat.Massless = true
	rplat.Parent = rhand
	rplat.Size = Vector3.new(1,1,0.1)
	
	button.MouseButton1Click:Connect(function()
		if on then
			on = false
			button.BackgroundColor3 = Color3.new(1,0.2,0.2)
		else
			on = true
			button.BackgroundColor3 = Color3.new(0.2,1,0.2)
		end
	end)
	
	while task.wait() do
		if on then
			
		end
		UIS.InputBegan:Connect(function(input)
			if input.KeyCode == Enum.KeyCode.ButtonX then
				if on then
					lplat.CFrame = lhand.CFrame:ToWorldSpace(offset)
				end
			end
			if input.KeyCode == Enum.KeyCode.ButtonA then
				if on then
					rplat.CFrame = rhand.CFrame:ToWorldSpace(offset)
				end
			end
		end)
		UIS.InputEnded:Connect(function(input)
			if input.KeyCode == Enum.KeyCode.ButtonX then
				lplat.CFrame = CFrame.new(0,-100,0)
			end
			if input.KeyCode == Enum.KeyCode.ButtonA then
				rplat.CFrame = CFrame.new(0,-100,0)
			end
		end)
	end
end
